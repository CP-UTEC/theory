#include <bits/stdc++.h>
using namespace std;
const int N = 1000005;
int n, q, a[N], cnt[N], res[N];
vector<pair<int, int>> queries[N];

void divisors(int x) {
}

int main() {
	cin >> n >> q;
	for (int i = 1; i <= n; i++) {
		cin >> a[i];
	}

	for (int i = 0; i < q; i++) {
		int m, p;
		cin >> m >> p;
		queries[m].push_back({p, i});
	}

	for (int i = 1; i <= n; i++) {
		divisors(a[i]);
		
		for (pair<int, int> qu : queries) {
			int p = qu.first;
			int idx = qu.second;
			res[idx] = cnt[p];
		}
	}

	return 0;
}
